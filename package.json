{
  "name": "@sassoftware/gpt-samples",
  "version": "1.6.1",
  "description": "Examples of integrating gpt prompts into SAS Viya apps",
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=9.7.2"
  },
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node --no-warnings cli",
    "basic": "cd ./packages/basic && node index.js",
    "basicsas": "cd ./packages/basicSAS && cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node index.js",
    "debug": "cd ./packages/gptViyaCli && cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node --inspect-brk cli.js",
    "pub": "npm publish --tag dev --access public"
  },
  "repository": "https://github.com/sassoftware/restaf-demos/tree/gpt-samples",
  "main": "./cli.js",
  "bin": {
    "@sassoftware/gpt-samples": "cli.js"
  },
  "keywords": [
    "Viya",
    "gpt",
    "gpt functions",
    "restaf",
    "SASREST API"
  ],
  "author": "Deva Kumar <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@sassoftware/restaf": "next",
    "@sassoftware/restafedit": "^2.4.4",
    "@sassoftware/restaflib": "next",
    "cross-env": "^7.0.3",
    "openai": "^4.26.0",
    "rimraf": "^3.0.2",
    "vorpal": "^1.12.0",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0"
  }
}
