{
  "name": "@sassoftware/openai-assistantjs",
  "version": "0.0.1",
  "description": "A library to access openau assistant",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.7.2"
  },
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node --no-warnings cli",
    "debug": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node --inspect-brk cli.js",
    "build": "microbundle --name assistantjs",
    "pub": "npm publish --tag dev --access public",
    "lint": "eslint . --ext .js --fix"
  },
  "repository": "https://github.com/sassoftware/restaf-demos/tree/openai-assistantjs",
  "source": "./src/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.modern.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "keywords": [
    "gpt",
    "gpt functions",
    "openai assistant",
    "SAS REST API",
    "restaf"
  ],
  "author": "Deva Kumar <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.11",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@sassoftware/restaf": "^5.2.4",
    "@sassoftware/restafedit": "^2.4.7",
    "@sassoftware/restaflib": "^5.2.4",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "openai": "^4.26.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-n": "^16.6.2",
    "microbundle": "^0.15.1"
  },
  "resolution": {
    "**/lodash": ">=4.17.2"
  }
}
