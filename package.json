{
  "name": "@sassoftware/gpt-samples",
  "version": "1.13.0",
  "description": "Examples of integrating gpt prompts into SAS Viya apps",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.7.2"
  },
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node --no-warnings cli",
    "basic": "cd ./packages/basic && node index.js",
    "basicsas": "cd ./packages/basicSAS && cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node index.js",
    "debug": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node --inspect-brk cli.js",
    "pub": "npm publish --tag dev --access public",
    "lint": "eslint . --ext .js --fix"
  },
  "repository": "https://github.com/sassoftware/restaf-demos/tree/gpt-samples",
  "main": "./cli.js",
  "bin": {
    "@sassoftware/gpt-samples": "cli.js"
  },
  "keywords": [
    "gpt",
    "gpt functions",
    "restaf",
    "SASREST API"
  ],
  "author": "Deva Kumar <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.11",
    "@sassoftware/restaf": "^5.2.4",
    "@sassoftware/restafedit": "^2.4.7",
    "@sassoftware/restaflib": "^5.2.4",
    "cross-env": "^7.0.3",
    "inquirer": "^9.2.15",
    "openai": "^4.26.0",
    "rimraf": "^3.0.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-react": "^7.33.2"
  },
  "resolution": {
    "**/lodash": ">=4.17.2"
  }
}
