{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {  
    "no-undef":"error",
    "no-unused-vars":"error",
    "no-multi-spaces":"error",
    "no-redeclare": "error",
    "no-throw-literal": "off",
    "no-return-await": "error"

  }
}